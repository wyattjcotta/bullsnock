
["Anti Shadow"]

	["Anti Shadow".config]
		#Erase the shadow of the sliding door by light value 1.
		antiShadow = true

["About explosion"]

	["About explosion".config]

		["About explosion".config.blastblockbreak]
			#Break blocks with ammunition blast is true.
			enabled = true

